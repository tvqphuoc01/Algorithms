#include<stdio.h>
int main(){
    int x1,v1,x2,v2;
    int t=1;
    scanf("%d %d %d %d",&x1,&v1,&x2,&v2);
    if((x1 > x2 && v1 > v2) || (x1 < x2 && v1 < v2) || (x1 == x2 ) || (v1 == v2)) {
        printf("NO");
    }
    else{
        
        if((-(x1-x2))%(v1-v2) == 0){
            printf("YES");
        }else{
            printf("NO");
        }
    }
    return 0;
}
