#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
 int main(){
     int n, arr[101]={};
     scanf("%d",&n);
     for(int i = 0; i < n;i++){
         int a;
         scanf("%d",&a);
        arr[a]++;
     }
     int sum = 0;
     for(int i=0;i<=100;i++){
         sum += arr[i]/2;
     }
     printf("%d", sum);
 }
